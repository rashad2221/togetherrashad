<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity"
    android:gravity="center">

    <RelativeLayout
        android:id="@+id/content_container"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">


        <RelativeLayout
            android:id="@+id/page_travel_5_ek1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="0dp"
            android:layout_marginTop="0dp">


            <View
                android:id="@+id/rectangle_1434"
                android:layout_width="419dp"
                android:layout_height="83dp"
                android:layout_marginStart="0dp"
                android:layout_marginTop="-7dp"
                android:background="@drawable/blue" />

            <ImageView
                android:id="@+id/logo"
                android:layout_width="161dp"
                android:layout_height="101dp"
                android:layout_alignBottom="@+id/textView14"
                android:layout_alignParentStart="true"
                android:layout_alignParentEnd="true"
                android:layout_marginStart="-15dp"
                android:layout_marginTop="8dp"
                android:layout_marginEnd="274dp"
                android:layout_marginBottom="607dp"
                android:background="@drawable/logofinal" />

            <RelativeLayout
                android:id="@+id/fullname"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="32dp"
                android:layout_marginTop="273dp">

                <View
                    android:id="@+id/emailBox"
                    android:layout_width="344dp"
                    android:layout_height="49dp"
                    android:layout_marginStart="0dp"
                    android:layout_marginTop="8dp"
                    android:background="@drawable/rectangle_4_shape" />


                <RelativeLayout
                    android:id="@+id/frame_8"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="16dp"
                    android:layout_marginTop="0dp" />

                <RelativeLayout
                    android:id="@+id/frame_8_ek1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_above="@+id/password"
                    android:layout_marginStart="16dp"
                    android:layout_marginTop="0dp"
                    android:layout_marginBottom="68dp" />


                <View
                    android:id="@+id/_bg__travel_5_ek2"
                    android:layout_width="414dp"
                    android:layout_height="896dp"
                    android:layout_marginStart="0dp"
                    android:layout_marginTop="0dp"
                    android:background="@color/white" />

                <View
                    android:id="@+id/passwordBox"
                    android:layout_width="344dp"
                    android:layout_height="49dp"
                    android:layout_marginStart="0dp"
                    android:layout_marginTop="8dp"
                    android:background="@color/white" />


                <EditText
                    android:id="@+id/email"

                    android:layout_width="320dp"
                    android:layout_height="59dp"
                    android:layout_alignEnd="@+id/emailBox"
                    android:layout_marginTop="-4dp"
                    android:layout_marginEnd="6dp"
                    android:layout_marginBottom="115dp"
                    android:background="@color/white"
                    android:ems="10"
                    android:hint="Email"
                    android:inputType="textEmailAddress"
                    android:textColor="@color/black"
                    android:textColorHint="@color/black" />

                <View
                    android:id="@+id/emailBox2"
                    android:layout_width="344dp"
                    android:layout_height="49dp"
                    android:layout_alignTop="@+id/password"
                    android:layout_alignBottom="@+id/password"
                    android:layout_marginStart="0dp"
                    android:layout_marginTop="22dp"
                    android:layout_marginBottom="-1dp"
                    android:background="@color/white" />

                <EditText
                    android:id="@+id/password"

                    android:layout_width="320dp"
                    android:layout_height="66dp"
                    android:layout_marginStart="16dp"
                    android:layout_marginTop="68dp"
                    android:layout_marginEnd="31dp"
                    android:layout_marginBottom="115dp"
                    android:ems="10"
                    android:hint="Password"
                    android:inputType="textEmailAddress"
                    android:textColorHint="@color/black" />

            </RelativeLayout>


            <RelativeLayout
                android:id="@+id/fullname_ek1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="32dp"
                android:layout_marginTop="345dp">


            </RelativeLayout>


            <RelativeLayout
                android:id="@+id/pills"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="57dp"
                android:layout_marginTop="455dp" />

            <TextView
                android:id="@+id/textView7"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignBottom="@+id/fullname_ek1"
                android:layout_alignParentEnd="true"
                android:layout_marginEnd="146dp"
                android:layout_marginBottom="271dp"
                android:text="together"
                android:textColor="#Fff6ea"
                android:textSize="40dp" />

            <TextView
                android:id="@+id/textView9"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignBottom="@+id/fullname"
                android:layout_alignParentEnd="true"
                android:layout_marginEnd="325dp"
                android:layout_marginBottom="507dp"
                android:text="Log In"
                android:textColor="#151414"
                android:textSize="20sp" />

            <TextView
                android:id="@+id/textView14"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignBottom="@+id/pills"
                android:layout_alignParentEnd="true"
                android:layout_marginEnd="176dp"
                android:layout_marginBottom="-228dp"
                android:text="" />

            <ImageButton
                android:id="@+id/sign_in"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignBottom="@+id/textView14"
                android:layout_alignParentEnd="true"
                android:layout_marginEnd="61dp"
                android:layout_marginBottom="186dp"
                android:background="@color/white"
                android:contentDescription="good"
                app:srcCompat="@drawable/login" />

        </RelativeLayout>
    </RelativeLayout>


</androidx.constraintlayout.widget.ConstraintLayout>
